{
  "listenAddress": {
    "host": "192.168.1.1",
    "port": "10053"
  },
  "remoteHTTPURLs": [
    "https://cloudflare-dns.com/dns-query"
  ],
  "forwardDomain": "domain.",
  "forwardNamesToAddresses": [
    {
      "name": "apu2.domain.",
      "ipAddress": "192.168.1.1"
    },
    {
      "name": "raspberrypi.domain.",
      "ipAddress": "192.168.1.100"
    },
    {
      "name": "oldpc.domain.",
      "ipAddress": "192.168.1.101"
    }
  ],
  "forwardResponseTTLSeconds": 60,
  "reverseDomain": "1.168.192.in-addr.arpa.",
  "reverseAddressesToNames": [
    {
      "reverseAddress": "1.1.168.192.in-addr.arpa.",
      "name": "apu2.domain."
    },
    {
      "reverseAddress": "100.1.168.192.in-addr.arpa.",
      "name": "raspberrypi.domain."
    },
    {
      "reverseAddress": "101.1.168.192.in-addr.arpa.",
      "name": "oldpc.domain."
    }
  ],
  "reverseResponseTTLSeconds": 60,
  "proxyConfiguration": {
    "minTTLSeconds": 300,
    "maxTTLSeconds": 3600,
    "removeEDNSPadding": true
  },
  "maxCacheSize": 20000,
  "timerIntervalSeconds": 60,
  "maxCachePurgesPerTimerPop": 100
}
